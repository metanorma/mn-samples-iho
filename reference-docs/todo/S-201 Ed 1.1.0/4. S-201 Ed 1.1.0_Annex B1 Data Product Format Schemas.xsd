			<xs:enumeration value="concreted">
				<xs:annotation>
					<xs:documentation>constructed of concrete, a material made of sand and gravel that is united by cement into a hardened mass used for roads, foundations, etc.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="loose boulders">
				<xs:annotation>
					<xs:documentation>constructed from large stones or blocks of concrete, often placed loosely for protection against waves or water turbulence.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="hard surface">
				<xs:annotation>
					<xs:documentation>constructed with a surface of hard material, usually a term applied to roads surfaced with asphalt or concrete.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="unsurfaced">
				<xs:annotation>
					<xs:documentation>constructed with no extra protection, usually a term applied to roads not surfaced with a hard material.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="wooden">
				<xs:annotation>
					<xs:documentation>constructed from wood.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="metal">
				<xs:annotation>
					<xs:documentation>constructed from metal.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="glass reinforced plastic (GRP)">
				<xs:annotation>
					<xs:documentation>constructed from a plastic material strengthened with fibres of glass.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="painted">
				<xs:annotation>
					<xs:documentation>the application of paint to some other construction or natural feature.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="fiberglass">
				<xs:annotation>
					<xs:documentation>Constructed from fiberglass</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="plastic">
				<xs:annotation>
					<xs:documentation>Constructed from plastic</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="productType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="oil">
				<xs:annotation>
					<xs:documentation>a thick, slippery liquid that will not dissolve in water, usually petroleum based in the context of storage tanks.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="gas">
				<xs:annotation>
					<xs:documentation>a substance with particles that can move freely, usually a fuel substance in the context of storage tanks.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="water">
				<xs:annotation>
					<xs:documentation>a colourless, odourless, tasteless liquid that is a compound of hydrogen and oxygen.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="stone">
				<xs:annotation>
					<xs:documentation>a general term for rock fragments.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="coal">
				<xs:annotation>
					<xs:documentation>a hard black mineral that is burned as fuel.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ore">
				<xs:annotation>
					<xs:documentation>a solid rock or mineral from which metal is obtained.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="chemicals">
				<xs:annotation>
					<xs:documentation>any substance obtained by or used in a chemical process.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="drinking water">
				<xs:annotation>
					<xs:documentation>water that is suitable for human consumption.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="milk">
				<xs:annotation>
					<xs:documentation>a white fluid secreted by female mammals as food for their young.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="bauxite">
				<xs:annotation>
					<xs:documentation>a mineral from which aluminum is obtained.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="coke">
				<xs:annotation>
					<xs:documentation>a solid substance obtained after gas and tar have been extracted from coal, used as a fuel.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="iron ingots">
				<xs:annotation>
					<xs:documentation>an oblong lump of cast iron metal.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="salt">
				<xs:annotation>
					<xs:documentation>sodium chloride obtained from mines or by the evaporation of sea water.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="sand">
				<xs:annotation>
					<xs:documentation>tiny grains of crushed or worn rock.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="timber">
				<xs:annotation>
					<xs:documentation>wood prepared for use in building or carpentry.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="sawdust/wood chips">
				<xs:annotation>
					<xs:documentation>powdery fragments of wood made in sawing timber or coarse chips produced for use in manufacturing pressed board.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="scrap metal">
				<xs:annotation>
					<xs:documentation>discarded metal suitable for being reprocessed.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="liquified natural gas (LNG)">
				<xs:annotation>
					<xs:documentation>a compressed gas consisting of flammable light hydrocarbons and derived from natural gas.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="liquified petroleum gas (LPG)">
				<xs:annotation>
					<xs:documentation>a compressed gas consisting of flammable light hydrocarbons and derived from petroleum.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="wine">
				<xs:annotation>
					<xs:documentation>the fermanted juice of grapes.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="cement">
				<xs:annotation>
					<xs:documentation>a substance made of powdered lime and clay, mixed with water.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="grain">
				<xs:annotation>
					<xs:documentation>a small hard seed, especially that of any cereal plant such as wheat, rice, corn, rye etc.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="qualityOfPositionType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="surveyed">
				<xs:annotation>
					<xs:documentation>the position(s) was(were) determined by the operation of making measurements for determining the relative position of points on, above or beneath the earth's surface. Survey implies a regular, controlled survey of any date.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="unsurveyed">
				<xs:annotation>
					<xs:documentation>survey data is does not exist or is very poor.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="inadequately surveyed">
				<xs:annotation>
					<xs:documentation>position data is of a very poor quality.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="approximate">
				<xs:annotation>
					<xs:documentation>a position that is considered to be less than third-order accuracy, but is generally considered to be within 30.5 metres of its correct geographic location. Also may apply to an object whose position does not remain fixed.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="position doubtful">
				<xs:annotation>
					<xs:documentation>an object whose position has been reported but which is considered to be doubtful.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="unreliable">
				<xs:annotation>
					<xs:documentation>an object's position obtained from questionable or unreliable data.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="reported (not surveyed)">
				<xs:annotation>
					<xs:documentation>an object whose position has been reported and its position confirmed by some means other than a formal survey such as an independent report of the same object.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="reported (not confirmed)">
				<xs:annotation>
					<xs:documentation>an object whose position has been reported and its position has not been confirmed.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="estimated">
				<xs:annotation>
					<xs:documentation>the most probable position of an object determined from incomplete data or data of questionable accuracy.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="precisely known">
				<xs:annotation>
					<xs:documentation>a position that is of a known value, such as the position of an anchor berth or other defined object.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="calculated">
				<xs:annotation>
					<xs:documentation>a position that is computed from data.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="qualityOfSoundingMeasurementType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="depth known">
				<xs:annotation>
					<xs:documentation>the depth from chart datum to the bottom is a known value.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="depth or least depth unknown">
				<xs:annotation>
					<xs:documentation>the depth from chart datum to the bottom is unknown.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="doubtful sounding">
				<xs:annotation>
					<xs:documentation>a depth that may be less than indicated.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="unreliable sounding">
				<xs:annotation>
					<xs:documentation>a depth that is considered to be an unreliable value.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="no bottom found at value shown">
				<xs:annotation>
					<xs:documentation>upon investigation the bottom was not found at this depth.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="least depth known">
				<xs:annotation>
					<xs:documentation>the shoalest depth over a feature is of known value.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="least depth unknown, safe clearance at depth shown">
				<xs:annotation>
					<xs:documentation>the least depth over a feature is unknown, but there is considered to be safe clearance at this depth.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="value reported (not surveyed)">
				<xs:annotation>
					<xs:documentation>depth value obtained from a report, but not fully surveyed.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="value reported (not confirmed)">
				<xs:annotation>
					<xs:documentation>depth value obtained from a report, which it has not been possible to confirm.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="maintained depth">
				<xs:annotation>
					<xs:documentation>the depth at which a channel is kept by human influence, usually by dredging.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="not regularly maintained">
				<xs:annotation>
					<xs:documentation>depths may be altered by human influence, but will not be routinely maintained.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="radarConspicuousType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="radar conspicuous">
				<xs:annotation>
					<xs:documentation>an object which returns a strong radar echo.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="not radar conspicuous">
				<xs:annotation>
					<xs:documentation>an object which does not return a particularly strong radar echo.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="radar conspicuous (has radar reflector)">
				<xs:annotation>
					<xs:documentation>an object which returns a strong radar echo, having a radar reflector.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="signalGenerationType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="automatically">
				<xs:annotation>
					<xs:documentation>signal generation is initiated by a self regulating mechanism such as a timer or light sensor.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="by wave action">
				<xs:annotation>
					<xs:documentation>the signal is generated by the motion of the sea surface such as a bell in a buoy.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="by hand">
				<xs:annotation>
					<xs:documentation>the signal is generated by a manually operated mechanism such as a hand cranked siren.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="by wind">
				<xs:annotation>
					<xs:documentation>the signal is generated by the motion of air such as a wind driven whistle.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="statusType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="permanent">
				<xs:annotation>
					<xs:documentation>intended to last or function indefinitely.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="occasional">
				<xs:annotation>
					<xs:documentation>acting on special occasions happening irregularly.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="recommended">
				<xs:annotation>
					<xs:documentation>presented as worthy of confidence, acceptance, use, etc.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="not in use">
				<xs:annotation>
					<xs:documentation>no longer used for the purpose intended disused.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="periodic/intermittent">
				<xs:annotation>
					<xs:documentation>recurring at intervals.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="reserved">
				<xs:annotation>
					<xs:documentation>set apart for some specific use.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="temporary">
				<xs:annotation>
					<xs:documentation>meant to last only for a time.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="private">
				<xs:annotation>
					<xs:documentation>not in public ownership or operation.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="mandatory">
				<xs:annotation>
					<xs:documentation>compulsory enforced.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="extinguished">
				<xs:annotation>
					<xs:documentation>no longer lit</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="illuminated">
				<xs:annotation>
					<xs:documentation>lit by floodlights, strip lights, etc.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="historic">
				<xs:annotation>
					<xs:documentation>famous in history of historical interest.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="public">
				<xs:annotation>
					<xs:documentation>belonging to, available to, used or shared by, the community as a whole and not restricted to private use.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="synchronized">
				<xs:annotation>
					<xs:documentation>occur at a time, coincide in point of time, be contemporary or simultaneous.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="watched">
				<xs:annotation>
					<xs:documentation>looked at or observed over a period of time especially so as to be aware of any movement or change.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="un-watched">
				<xs:annotation>
					<xs:documentation>usually automatic in operation, without any permanently-stationed personnel to superintend it.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="existence doubtful">
				<xs:annotation>
					<xs:documentation>an object that has been reported but has not been definitely determined to exist.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="confirmed">
				<xs:annotation>
					<xs:documentation>made certain as to truth, accuracy, validity, availability, etc.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="candidate">
				<xs:annotation>
					<xs:documentation>Item selected for an action</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="under modification">
				<xs:annotation>
					<xs:documentation>Item that is in the process of being modified</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="candidate for modification">
				<xs:annotation>
					<xs:documentation>Item selected for modification</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="under removal/deletion">
				<xs:annotation>
					<xs:documentation>Item in the process of being removed or deleted</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="removed/deleted">
				<xs:annotation>
					<xs:documentation>Item that has been removed or deleted</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="techniqueOfSoundingMeasurementType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="found by echo-sounder">
				<xs:annotation>
					<xs:documentation>the depth was determined by using an instrument that determines depth of water by measuring the time interval between emission of a sonic or ultrasonic signal and return of its echo from the bottom.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="found by side-scan-sonar">
				<xs:annotation>
					<xs:documentation>the depth was computed from a record produced by active sonar in which fixed acoustic beams are directed into the water perpendicularly to the direction of travel to scan the bottom and generate a record of the bottom configuration.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="found by multi-beam">
				<xs:annotation>
					<xs:documentation>the depth was determined by using a wide swath echo sounder that uses multiple beams to measure depths directly below and transverse to the ship's track.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="found by diver">
				<xs:annotation>
					<xs:documentation>the depth was determined by a person skilled in the practice of diving.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="found by lead-line">
				<xs:annotation>
					<xs:documentation>the depth was determined by using a line, graduated with attached marks and fastened to a sounding lead.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="swept by wire-drag">
				<xs:annotation>
					<xs:documentation>the given area was determined to be free from navigational dangers to a certain depth by towing a buoyed wire at the desired depth by two launches, or a least depth was identified using the same technique.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="found by laser">
				<xs:annotation>
					<xs:documentation>the depth was determined by using an instrument that measures distance by emitting timed pulses of laser light and measuring the time between emission and reception of the reflected pulses.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="swept by vertical acoustic system">
				<xs:annotation>
					<xs:documentation>the given area has been swept using a system comprised of multiple echo sounder transducers attached to booms deployed from the survey vessel.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="found by electromagnetic sensor">
				<xs:annotation>
					<xs:documentation>the depth was determined by using an instrument that compares electromagnetic signals.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="photogrammetry">
				<xs:annotation>
					<xs:documentation>the depth was determined by applying mathematical techniques to photographs.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="satellite imagery">
				<xs:annotation>
					<xs:documentation>the depth was determined by using instruments placed aboard an artificial satellite.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="found by levelling">
				<xs:annotation>
					<xs:documentation>the depth was determined by using levelling techniques to find the elevation of the point relative to a datum.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="swept by side-scan-sonar">
				<xs:annotation>
					<xs:documentation>the given area was determined to be free from navigational dangers to a certain depth by towing a side-scan-sonar.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="computer generated">
				<xs:annotation>
					<xs:documentation>the sounding was determined from a bottom model constructed using a computer.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="topmarkDaymarkShapeType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="cone, point up">
				<xs:annotation>
					<xs:documentation>is where the vertex points up. A cone is a solid figure generated by straight lines drawn from a fixed point (the vertex) to a circle in a plane not containing the vertex. Cones are commonly used as International Association of Lighthouse Authorities - IALA topmarks lateral.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="cone, point down">
				<xs:annotation>
					<xs:documentation>is where the vertex points down. A cone is a solid figure generated by straight lines drawn from a fixed point (the vertex) to a circle in a plane not containing the vertex. Cones are commonly used as International Association of Lighthouse Authorities - IALA topmarks lateral.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="sphere">
				<xs:annotation>
					<xs:documentation>a body the surface of which is at all points equidistant from the centre. Spheres are commonly used as International Association of Lighthouse Authorities - IALA topmarks safe water.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2 spheres">
				<xs:annotation>
					<xs:documentation>two black spheres are commonly used as an International Association of Lighthouse Authorities - IALA topmark (isolated danger).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="cylinder (can)">
				<xs:annotation>
					<xs:documentation>a solid geometrical figure generated by straight lines fixed in direction and describing with one of point a closed curve, especially a circle (in which case the figure is circular cylinder, it's ends being parallel circles). Cylinders are commonly used as International Association of Lighthouse Authorities - IALA topmarks lateral.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="board">
				<xs:annotation>
					<xs:documentation>usually of rectangular shape, made from timber or metal and used to provide a contrast with the natural background of a daymark. The actual daymark is often painted on to this board.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="x-shape (St. Andrew's cross)">
				<xs:annotation>
					<xs:documentation>having a shape or a cross-section like the capital letter X. An x-shape as an International Association of Lighthouse Authorities - IALA topmark should be 3 dimensional in shape. It is made of at least three crossed bars.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="upright cross (St. George's Cross)">
				<xs:annotation>
					<xs:documentation>a cross with one vertical member and one horizontal member, i.e. similar in shape to the character '+'.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="cube, point up">
				<xs:annotation>
					<xs:documentation>a cube standing on one of its vertexes. A cube is a solid contained by six equal squares a regular hexahedron (The New Shorter Oxford English Dictionary. 1993. vol 2)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2 cones, point to point">
				<xs:annotation>
					<xs:documentation>2 cones, one above the other, with their vertices together in the centre.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2 cones, base to base">
				<xs:annotation>
					<xs:documentation>2 cones, one above the other, with their bases together in the centre and their vertices pointing up and down.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="rhombus">
				<xs:annotation>
					<xs:documentation>a plane figure having four equal sides and equal opposite angles (two acute and two obtuse) an oblique equilateral parallelogram.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2 cones (points upward)">
				<xs:annotation>
					<xs:documentation>2 cones, one above the other, with their vertices pointing up.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2 cones (points downward)">
				<xs:annotation>
					<xs:documentation>2 cones, one above the other, with their vertices pointing down.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="besom, point up (broom or perch)">
				<xs:annotation>
					<xs:documentation>besom: a bundle of rods or twigs. perch: a staff placed on top of a buoy, rock or shoal as a mark for navigation.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="besom, point down (broom or perch)">
				<xs:annotation>
					<xs:documentation>besom: a bundle of rods or twigs. perch: a staff placed on top of a buoy, rock or shoal as a mark for navigation.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="flag">
				<xs:annotation>
					<xs:documentation>a flag mounted on a short pole.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="sphere over rhombus">
				<xs:annotation>
					<xs:documentation>A sphere located above a rhombus.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="square">
				<xs:annotation>
					<xs:documentation>a plane figure with four right angles and four equal straight sides</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="rectangle, horizontal">
				<xs:annotation>
					<xs:documentation>a rectangle is a plane figure with four right angles and four straight sides, opposite sides being parallel and equal in length . A horizontal rectangle is where the two longer opposite sides are standing horizontally.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="rectangle, vertical">
				<xs:annotation>
					<xs:documentation>a rectangle is a plane figure with four right angles and four straight sides, opposite sides being parallel and equal in length. A vertical rectangle is where the two longer opposite sides are standing vertically.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="trapezium, up">
				<xs:annotation>
					<xs:documentation>which stands on its longer parallel side. A trapezium is a quadrilateral having one pair of opposite sides parallel.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="trapezium, down">
				<xs:annotation>
					<xs:documentation>which stands on its shorter parallel side. A trapezium is a quadrilateral having one pair of opposite sides parallel.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="triangle, point up">
				<xs:annotation>
					<xs:documentation>A triangle, point up. A triangle is a figure having three angles and three sides.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="triangle, point down">
				<xs:annotation>
					<xs:documentation>A triangle, point down. A triangle is a figure having three angles and three sides.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="circle">
				<xs:annotation>
					<xs:documentation>a perfectly round plane figure whose circumference is everywhere equidistant from its centre.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="two upright crosses (one over the other)">
				<xs:annotation>
					<xs:documentation>two upright crosses, generally vertically disposed one above the other.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="T-shape">
				<xs:annotation>
					<xs:documentation>having a shape like the capital letter T.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="triangle pointing up over a circle">
				<xs:annotation>
					<xs:documentation>a triangle, vertex uppermost, located above a circle.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="upright cross over a circle">
				<xs:annotation>
					<xs:documentation>an upright cross located above a circle.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="rhombus over a circle">
				<xs:annotation>
					<xs:documentation>a rhombus located above a circle.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="circle over a triangle pointing up">
				<xs:annotation>
					<xs:documentation>a circle located over a triangle, vertex uppermost.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="other shape (see INFORM)">
				<xs:annotation>
					<xs:documentation/>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="tubular">
				<xs:annotation>
					<xs:documentation>Having the form of or consisting of a tube.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="trafficFlowType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="inbound">
				<xs:annotation>
					<xs:documentation>traffic flow in a general direction toward a port or similar destination.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="outbound">
				<xs:annotation>
					<xs:documentation>traffic flow in a general direction away from a port or similar point of origin.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="one-way">
				<xs:annotation>
					<xs:documentation>traffic flow in one general direction only.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="two-way">
				<xs:annotation>
					<xs:documentation>traffic flow in two generally opposite directions.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="verticalDatumType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="mean low water springs">
				<xs:annotation>
					<xs:documentation>(MLWS) - the average height of the low waters of spring tides.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="mean lower low water springs">
				<xs:annotation>
					<xs:documentation>(MLLWS) - the average height of lower low water springs at a place.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="mean sea level">
				<xs:annotation>
					<xs:documentation>(MSL) - the average height of the surface of the sea at a tide station for all stages of the tide over a 19-year period, usually determined from hourly height readings measured from a fixed predetermined reference level.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="lowest low water">
				<xs:annotation>
					<xs:documentation>an arbitrary level conforming to the lowest tide observed at a place, or some what lower.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="mean low water">
				<xs:annotation>
					<xs:documentation>(MLW) - the average height of all low waters at a place over a 19-year period.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="lowest low water springs">
				<xs:annotation>
					<xs:documentation>an arbitrary level conforming to the lowest water level observed at a place at spring tides during a period of time shorter than 19 years.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="approximate mean low water springs">
				<xs:annotation>
					<xs:documentation>an arbitrary level, usually within ' 0.3m from that of mean low water springs (MLWS).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Indian spring low water">
				<xs:annotation>
					<xs:documentation>(ISLW) - an arbitrary tidal datum approximating the level of the mean of the lower low water at spring tides.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="low water springs">
				<xs:annotation>
					<xs:documentation>an arbitrary level, approximating that of mean low water springs (MLWS).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="approximate lowest astronomical tide">
				<xs:annotation>
					<xs:documentation>an arbitrary level, usually within ' 0.3m from that of lowest astronomical tide (LAT).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="nearly lowest low water">
				<xs:annotation>
					<xs:documentation>an arbitrary level approximating the lowest water level observed at a place, usually equivalent to the Indian spring low water (ISLW).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="mean lower low water">
				<xs:annotation>
					<xs:documentation>(MLLW) - the average height of the lower low waters at a place over a 19-year period.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="low water">
				<xs:annotation>
					<xs:documentation>an approximation of mean low water adopted as the reference level for a limited area, irrespective of better determinations at a later date.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="approximate mean low water">
				<xs:annotation>
					<xs:documentation>an arbitrary level, usually within ' 0.3m from that of mean low water (MLW).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="approximate mean lower low water">
				<xs:annotation>
					<xs:documentation>an arbitrary level, usually within ' 0.3m from that of mean lower low water (MLLW).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="mean high water">
				<xs:annotation>
					<xs:documentation>(MHW) - the average height of all high waters at a place over a 19-year period.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="mean high water springs">
				<xs:annotation>
					<xs:documentation>(MHWS) - the average height of the high waters of spring tides.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="high water">
				<xs:annotation>
					<xs:documentation>the highest level reached at a place by the water surface in one tidal cycle.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="approximate mean sea level">
				<xs:annotation>
					<xs:documentation>an arbitrary level, usually within ' 0.3m from that of mean sea level (MSL).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="high water springs">
				<xs:annotation>
					<xs:documentation>an arbitrary level, approximating that of mean high water springs (MHWS).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="mean higher high water">
				<xs:annotation>
					<xs:documentation>(MHHW) - the average height of higher high waters at a place over a 19-year period.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="equinoctial spring low water">
				<xs:annotation>
					<xs:documentation>the level of low water springs near the time of an equinox.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="lowest astronomical tide">
				<xs:annotation>
					<xs:documentation>(LAT) - the lowest tide level which can be predicted to occur under average meterological conditions and under any combination of astronomical conditions.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="local datum">
				<xs:annotation>
					<xs:documentation>an arbitrary datum defined by a local harbour authority, from which levels and tidal heights are measured by this authority.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="international great lakes datum 1985">
				<xs:annotation>
					<xs:documentation>(IGLD 1985) - a vertical reference system with its zero based on the mean water level at Rimouski/Pointe-au-P??re, Quebec, over the period 1970 to 1988.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="mean water level">
				<xs:annotation>
					<xs:documentation>the average of all hourly water levels over the available period of record.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="lower low water large tide">
				<xs:annotation>
					<xs:documentation>(LLWLT) - the average of the lowest low waters, one from each of 19 years of observations.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="higher high water large tide">
				<xs:annotation>
					<xs:documentation>(HHWLT) - the average of the highest high waters, one from each of 19 years of observations.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="nearly highest high water">
				<xs:annotation>
					<xs:documentation>an arbitrary level approximating the highest water level observed at a place, usually equivalent to the high water springs.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="highest astronomical tide (HAT)">
				<xs:annotation>
					<xs:documentation>the highest tidal level which can be predicted to occur under average meteorological conditions and under any combination of astronomical conditions.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="visuallyConspicuousType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="visually conspicuous">
				<xs:annotation>
					<xs:documentation>term applied to an object either natural or artificial which is distinctly and notably visible from seaward.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="not visually conspicuous">
				<xs:annotation>
					<xs:documentation>an object which is visible from seaward, but is not conspicuous.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<!-- ========================================================================================== -->
	<!-- S100_CodeList                                                                              -->
	<!-- ========================================================================================== -->
	<xs:simpleType name="categoryOfAggregationType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="leading line">
				<xs:annotation>
					<xs:documentation>A line passing through two or more clearly defined charted objects, and along which a vessel can approach safely.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="range system">
				<xs:annotation>
					<xs:documentation>Two or more objects in line. Such objects are said to be in range. An observer having them in range is said to be on the range.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="measured distance">
				<xs:annotation>
					<xs:documentation>A course at sea, whose ends are indicated by ranges ashore, and whose length has been accurately measured for determining the speed of vessels.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="categoryOfAssociationType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="channel markings">
				<xs:annotation>
					<xs:documentation>A group of channel marks which indicate channel limits</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="danger markings">
				<xs:annotation>
					<xs:documentation>One or more aids to navigation and the danger(s) they mark</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<!-- ========================================================================================== -->
	<!-- types and elements for the complex attribute types definition                              -->
	<!-- ========================================================================================== -->
	<xs:complexType name="changeDetailsType">
		<xs:annotation>
			<xs:documentation>.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="buoyChange" type="buoyChangeType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The characteristics of the buoy have been or will be changed.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lightChange" type="lightChangeType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The characteristics of the light have been or will be changed.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="beaconChange" type="beaconChangeType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The characteristics of the beacon have been or will be changed.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="leadingLightsChange" type="leadingLightsChangeType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fogSignals" type="fogSignalsType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>A warning signal transmitted by a vessel, or aid to navigation, during periods of low visibility. Also, the device producing such a signal.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="radioAidsChange" type="radioAidsChangeType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="atonCommissioning" type="atonCommissioningType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="atonReplacement" type="atonReplacementType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="atonRemoval" type="atonRemovalType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="contactAddressType">
		<xs:annotation>
			<xs:documentation>Direction or superscription of a letter, package, etc., specifying the name of the place to which it is directed, and optionally a contact person or organisation who should receive it. (Oxford English Dictionary, 2nd Ed., adapted).</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="deliveryPoint" type="xs:string" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Details  of where post can be delivered such as the apartment, name and/or number of a street, building or PO Box.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cityName" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The name of a town or city.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="administrativeDivision" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Administrative division is a generic term for an administrative region within a country at a level below that of the sovereign state.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="country" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The name of a nation. (Adapted from The American Heritage Dictionaries)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="postalCode" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Known in various countries as a postcode, or ZIP code, the postal code is a series of letters and/or digits that identifies each postal delivery area.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="surveyDateRangeType">
		<xs:annotation>
			<xs:documentation>The complex attribute describes the period of the hydrographic survey, as the time between its sub-attributes.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="dateEnd" type="S100_TruncatedDate" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The latest date on which an object (e.g., a buoy) will be present.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dateStart" type="S100_TruncatedDate" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The earliest date on which an object (e.g., a buoy) will be present.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
