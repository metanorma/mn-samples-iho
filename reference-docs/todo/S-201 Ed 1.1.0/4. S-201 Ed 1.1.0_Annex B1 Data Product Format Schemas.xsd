				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="SpatialUncertainty" type="SpatialUncertaintyType"/>
	<xs:complexType name="SpatialUncertaintyType">
		<xs:annotation>
			<xs:documentation/>
		</xs:annotation>
